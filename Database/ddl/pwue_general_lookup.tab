  DATA_VALUE   VARCHAR2(50) not null,
  DATA_DESC    VARCHAR2(150) not null,
  UPDT_USERID  VARCHAR2(30) default user,
  TIME_STAMP   DATE default sysdate,
  DATA_VALUE_1 VARCHAR2(150)
