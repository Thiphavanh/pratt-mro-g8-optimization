     <!--  work order create Over Haul -->
     <!-- Start -->
     <bean class="com.pw.solumina.sfpl.application.impl.OrderMROOverhaulPW"></bean>
	 <bean class="com.pw.solumina.sfpl.dao.impl.OrderCreateDaoPW"></bean>
	 <bean class="com.pw.solumina.sfpl.dao.impl.BuildMROOrderNumberDao"></bean>
     <bean class="com.pw.solumina.sfpl.application.impl.buildMROOrderNumber"></bean>
     <bean class="com.pw.solumina.sfpl.application.impl.UpdateOverHaulTaskStatus"></bean>
     
     
     <bean id="CallOrderCreate"
              class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
              <property name="transactionManager" ref="transactionManager" />
              <property name="transactionAttributes">
                      <props>
                      		<prop key="callOOBCreateOrder">PROPAGATION_REQUIRES_NEW</prop>   
                      </props>
              </property>
              <property name="target">
                      <bean class="com.pw.solumina.sfpl.application.impl.CallOrderCreate"> </bean>
              </property>
      </bean>
      
     <bean id="LogOrderCreateErrors"
              class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
              <property name="transactionManager" ref="transactionManager" />
              <property name="transactionAttributes">
                      <props>
                      		<prop key="insertIntoErrorTable">PROPAGATION_REQUIRES_NEW</prop>
                      </props>
              </property>
              <property name="target">
                      <bean class="com.pw.solumina.sfpl.dao.impl.LogOrderCreateErrors"> </bean>
                      
              </property>
      </bean>
     <!-- End -->      
      
     <!--  work order create Over Repair-->
     <!-- Start -->
     
      <!-- Start - Bean definitions for Asynchronous transaction config -->
      <bean id="asipsAsyncTransactionDelayZero" class="java.lang.Long">
              <constructor-arg value="0" />
      </bean>

      <bean id="asipsAsyncTransactionService"
              class="com.ibaset.common.concurrent.AsynchronousTransactionServiceImpl"
              destroy-method="cleanUp">
              <property name="poolSize" value="10" />
              <property name="login" ref="login" />
              <property name="errorLog" ref="errorLog" />
              <property name="processLog" ref="processLog" />
              <property name="delay" ref="asipsAsyncTransactionDelayZero" />
    <!--      <property name="transactionManager" ref="soluminaTransactionManager" />  -->
       </bean>
       <!-- End - Bean definitions for Asynchronous transaction config -->
     
     
     
