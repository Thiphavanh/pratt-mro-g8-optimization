<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:jms="http://www.springframework.org/schema/jms"
	   xmlns:aop="http://www.springframework.org/schema/aop"
	   xmlns:tx="http://www.springframework.org/schema/tx"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd 
       http://www.springframework.org/schema/jms http://www.springframework.org/schema/jms/spring-jms-2.1.xsd
       http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd"
	   default-lazy-init="false" default-dependency-check="none"
	   default-autowire="no">
	
	<!-- ALL BEAN DEFINITIONS THAT DEPENDS ON BEANS IN integrationContext.xml SHOULD GO IN integrationExtensionContext.xml -->
	
     <!-- !!! README README README !!! -->
     <!--  
     		You should not create beans for Interface classes or Abstract Classes.
     		Classes that are just data objects do not need to exist in this file either.
     		
     		Currently all classes should exist in this file, unnecessary beans have been commented out 
     		to prevent developers from recreating it and causing confusion. 
     -->
     <!-- !!! README README README !!! -->
	
	 <!-- PWMRO-Execution BEGIN -->

	 <!-- com.pw.solumina.sfwid.application -->
     <bean class="com.pw.solumina.sfwid.application.BuyoffPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.OperationPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.OrderPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.DeliveryPackagePW"></bean>
     <bean class="com.pw.solumina.sfwid.application.BarcodedTravelerPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.OperationDataCollectionPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.ROMHistoryReportPW"></bean>   
     <bean class="com.ibaset.solumina.sfwid.application.impl.OrderMroRepairSplitPW"></bean>
     <bean class="com.ibaset.solumina.sfwid.dao.impl.OrderMroRepairSplitDao"></bean>
     <bean class="com.ibaset.solumina.sfwid.dao.impl.RollUpDaoImplPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.StepPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.DataCollectionPW"></bean>
     
     
     <!-- com.pw.solumina.sfwid.application.impl -->
     <bean class="com.pw.solumina.sfwid.application.impl.SfwidOrderImplPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.impl.AlterationImplPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.impl.SerialOperationImplPW"></bean>
     <bean class="com.pw.solumina.sfwid.application.impl.OperationImplPW"></bean>
     
     <!-- com.pw.solumina.sfpl.application.impl -->
     <bean class="com.pw.solumina.sfpl.application.impl.OperationImplPW"></bean>
     <bean class="com.pw.solumina.sfpl.application.impl.FileImplPW"></bean>
     
     
	 <!-- com.pw.solumina.sfwid.dao -->
	 <bean class="com.pw.solumina.sfwid.dao.OrderAssembleTextDaoPW"></bean>
	 <bean class="com.pw.solumina.sfwid.dao.BuyoffDaoPW"></bean>
	 <bean class="com.pw.solumina.sfwid.dao.PartDaoPW"></bean>
	 <bean class="com.pw.solumina.sfwid.dao.UserDaoPW"></bean>
	 <bean class="com.pw.solumina.sfwid.dao.OperationDaoPW"></bean>
	 <bean class="com.pw.solumina.sfwid.dao.OrderDaoPW"></bean>
     <bean class="com.pw.solumina.sfwid.dao.WorkOrderDaoPW"></bean>	 
     <bean class="com.pw.solumina.sfwid.dao.OperationDataCollectionDaoPW"></bean>
     <bean class="com.pw.solumina.sfwid.dao.DataCollectionDaoPW"></bean>
     <bean class="com.pw.solumina.sfwid.dao.BarcodedTravelerDaoPW"></bean>
     <bean class="com.pw.solumina.sfwid.dao.ROMHistoryReportDaoPW"></bean>	 
     <bean class="com.pw.solumina.sfor.dao.AuditSubjectDaoPW"></bean>
     <bean class="com.pw.solumina.sfwid.dao.StepDaoPW"></bean>
     
     <!-- com.pw.solumina.solumina.sfrpt.impl -->
     <bean class="com.pw.solumina.solumina.sfrpt.impl.ReportImplPW"></bean>
     <bean class="com.pw.solumina.solumina.sfrpt.dao.impl.ReportDaoImplPW"></bean>     
     
     <bean id="ReleaseOrderAndCommitPW"
              class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
              <property name="transactionManager" ref="transactionManager" />
              <property name="transactionAttributes">
                      <props>
                      		<prop key="releaseOrder">PROPAGATION_REQUIRES_NEW</prop>   
                      </props>
              </property>
              <property name="target">
                      <bean class="com.pw.solumina.sfwid.application.ReleaseOrderAndCommitPW"> </bean>
              </property>
      </bean>      
     	 
    <!-- Cockpit Delivery Installation Beans -->
    <bean id="pwZcomSalesOrder" class ="com.pw.solumina.sfwid.application.impl.PWMROIZcomSalesOrderImpl"></bean>
	<bean id="pwZcomSalesOrderDao" class ="com.pw.solumina.sfwid.dao.impl.PWMROZcomSalesOrdersDaoImpl"></bean>
	<bean id="pwSerials" class = "com.pw.solumina.sfwid.application.impl.PWMROISerialImpl"></bean>
	<bean id="pwSerialsDao" class ="com.pw.solumina.sfwid.dao.impl.PWMROISerialDaoImpl"></bean>
      
     	 
	 <!-- PWMRO-Execution END -->

</beans>
